VITE_UE5_SERVER_URL=ws://localhost:8888

# Production UE5 Server (when deployed)
# VITE_UE5_SERVER_URL=wss://your-ue5-server.com:8888

# Performance Settings
VITE_LAZY_LOAD_GAMES=true
VITE_ENABLE_COMPRESSION=true
